server {
        listen 80;
        server_name 94.74.67.23;


        location /media {
                alias /home/demarq/.virtualenvs/lab6/lab6/lab5/media;
        }

        location /static {
                alias /home/demarq/.virtualenvs/lab6/lab6/lab5/static;
                add_header Cache-Control public;
                expires    max;
        }

        # Add index.php to the list if you are using PHP
        index index.php index.html index.htm index.nginx-debian.html;

        server_name _;

        location / {

                uwsgi_pass django;
                include /home/demarq/.virtualenvs/lab6/lab6/uwsgi_params;

        }

        error_page 404 /404.html;
        error_page 500 502 503 504 /50x.html;

        location = /50x.html {
                root /usr/share/nginx/html;
        }

}
